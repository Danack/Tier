{
    "name": "danack/tier",
    "description": "An 'framework' application using DI with tiers for full 'Dependency inversion'.",
    "keywords": ["DI", "Auryn", "application", "example"],

    "license": "MIT",
    "authors": [
        {
            "name": "Dan Ackroyd",
            "email": "Danack@basereality.com",
            "homepage": "http://www.basereality.com"
        }
    ],
    "autoload": {
        "psr-0": {
            "Tier": "src/"
