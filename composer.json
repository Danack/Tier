    "require-dev": {
        "danack/jig": "^0.21.2",
        "mockery/mockery": "0.9.1",
        "nikic/fast-route": "0.3.0",
        "phpunit/phpunit": "3.7.*",
        "room11/caching": "^0.0.4", 
        "room11/http": "^0.2.0",
        "squizlabs/php_codesniffer": "^2.0.0"
        
    },
    "require":{
        "php": "^5.5 | ^7",
        "rdlowrey/auryn": "^1.4.0",
        "danack/aurynconfig": "^0.9.1",
        "zendframework/zend-diactoros": "^1.3.2"
    }
}